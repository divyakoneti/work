<div class = "container" >
	<h3>Edit_catagory</h3>
	<%= form_for @catagory do |f| %>
		<div class = "form-group " >
			<%= f.label :name, class:"control-label " %>
			<%= f.text_field :name ,class:"form-control"%>
		</div>
		
		<div class="form-group">
		    <%= f.submit "save",class: "btn btn-primary" %>
		</div>  
	<% end %>
</div>		      	